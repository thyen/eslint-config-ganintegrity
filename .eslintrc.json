{
    "env": {
      "browser": true,
      "mocha": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "angular": false
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
      "curly": ["error", "all"],
      "prettier/prettier": ["error", {"singleQuote": true}]
    }
}
